{
  "name": "ricwein/templater",
  "description": "simple twig alike templating engine",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Richard Weinhold",
      "email": "git@ricwein.com"
    }
  ],
  "require": {
    "php": ">= 7.4.0",
    "matthiasmullie/minify": "^1.3",
    "phpfastcache/phpfastcache": "^7.1",
    "ricwein/filesystem": "^2.1",
    "ricwein/tokenizer": "^1.2",
    "scssphp/scssphp": "^1.0"
  },
  "require-dev": {
    "overtrue/phplint": "^1.2",
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ricwein\\Templater\\": "src/"
    }
  }
}
